<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentMain">

    <fragment
        android:id="@+id/fragmentMain"
        android:name="com.example.whereismybike.FragmentMain"
        android:label="fragment_fragment_main"
        tools:layout="@layout/fragment_fragment_main" >
        <action
            android:id="@+id/action_fragmentMain_to_fragmentSettings"
            app:destination="@id/fragmentSettings" />
    </fragment>
    <fragment
        android:id="@+id/fragmentSettings"
        android:name="com.example.whereismybike.FragmentSettings"
        android:label="fragment_fragment_settings"
        tools:layout="@layout/fragment_fragment_settings" />
    <fragment
        android:id="@+id/fragmentTakePicture"
        android:name="com.example.whereismybike.FragmentTakePicture"
        android:label="fragment_fragment_take_picture"
        tools:layout="@layout/fragment_fragment_take_picture" >
        <action
            android:id="@+id/action_fragmentTakePicture_to_fragmentSavePicture"
            app:destination="@id/fragmentSavePicture" />
    </fragment>
    <fragment
        android:id="@+id/fragmentSavePicture"
        android:name="com.example.whereismybike.FragmentSavePicture"
        android:label="fragment_fragment_save_picture"
        tools:layout="@layout/fragment_fragment_save_picture" >
        <action
            android:id="@+id/action_fragmentSavePicture_to_fragmentTakePicture2"
            app:destination="@id/fragmentTakePicture" />
    </fragment>
</navigation>